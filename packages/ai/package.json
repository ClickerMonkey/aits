{
  "name": "@aits/ai",
  "version": "0.1.0",
  "description": "@aits AI - AI TypeScript library main functionality",
  "type": "module",
  "main": "dist/index.js",
  "types": "dist/index.d.ts",
  "exports": {
    ".": {
      "source": "./src/index.ts",
      "import": "./dist/index.js",
      "require": "./dist/index.js",
      "types": "./dist/index.d.ts",
      "default": "./src/index.ts"
    }
  },
  "scripts": {
    "build": "tsc",
    "test": "jest",
    "test:coverage": "jest --coverage",
    "test:watch": "jest --watch",
    "clean": "rm -rf dist",
    "typecheck": "tsc --noEmit"
  },
  "keywords": [
    "ai",
    "typescript",
    "core"
  ],
  "author": "ClickerMonkey",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "https://github.com/ClickerMonkey/aits.git",
    "directory": "packages/ai"
  },
  "files": [
    "dist",
    "README.md",
    "LICENSE"
  ],
  "dependencies": {
    "@aits/core": "^0.1.0"
  },
  "devDependencies": {
    "@types/jest": "^30.0.0",
    "@types/node": "^24.9.1",
    "jest": "^30.2.0",
    "ts-jest": "^29.4.5",
    "typescript": "^5.0.0"
  },
  "publishConfig": {
    "access": "public"
  },
  "engines": {
    "node": ">=16.0.0"
  },
  "homepage": "https://github.com/ClickerMonkey/aits#readme",
  "bugs": {
    "url": "https://github.com/ClickerMonkey/aits/issues"
  }
