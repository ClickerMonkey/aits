{
  "name": "@aeye/core",
  "version": "0.3.2",
  "description": "@aeye Core - Core primitives for AI components (agents, tools, prompts)",
  "type": "module",
  "main": "dist/index.js",
  "types": "dist/index.d.ts",
  "exports": {
    ".": {
      "browser": {
        "import": "./dist/browser.js",
        "types": "./dist/browser.d.ts"
      },
      "default": {
        "import": "./dist/index.js",
        "types": "./dist/index.d.ts"
      }
    }
  },
  "scripts": {
