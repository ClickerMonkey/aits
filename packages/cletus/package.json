{
  "name": "@aeye/cletus",
  "version": "0.1.0",
  "description": "Interactive CLI demo showcasing AITS capabilities",
  "type": "module",
  "main": "dist/index.js",
  "types": "dist/index.d.ts",
  "bin": {
    "cletus": "./dist/index.js"
  },
  "scripts": {
    "build": "node esbuild.config.cjs",
    "dev": "tsx watch src/index.tsx",
    "start": "tsx --conditions=source src/index.tsx",
    "clean": "rimraf dist tsconfig.tsbuildinfo",
    "typecheck": "tsc --noEmit",
    "prepublishOnly": "npm run build"
  },
  "keywords": [
    "cli",
    "ai",
    "demo",
    "interactive"
  ],
  "author": "ClickerMonkey",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "https://github.com/ClickerMonkey/aeye.git",
    "directory": "packages/cletus"
  },
  "files": [
    "dist",
    "README.md",
    "LICENSE"
  ],
  "dependencies": {
    "@aeye/ai": "^0.1.0",
    "@aeye/aws": "^0.1.0",
    "@aeye/core": "^0.1.0",
    "@aeye/models": "^0.1.0",
    "@aeye/openai": "^0.1.0",
    "@aeye/openrouter": "^0.1.0",
    "@aeye/replicate": "^0.1.0",
    "@langchain/core": "^0.3.79",
    "@langchain/textsplitters": "^0.1.0",
    "@tavily/core": "^0.5.13",
    "diff": "^8.0.2",
    "file-type": "^21.0.0",
    "glob": "^11.0.0",
    "handlebars": "^4.7.8",
    "ink": "^6.4.0",
    "ink-big-text": "^2.0.0",
    "ink-gradient": "^3.0.0",
    "ink-select-input": "^6.2.0",
    "ink-syntax-highlight": "^2.0.2",
    "ink-text-input": "^6.0.0",
    "mammoth": "^1.9.1",
    "mic": "^2.1.2",
    "node-html-markdown": "^1.3.0",
    "node-poppler": "^8.0.4",
    "pdf-parse": "^1.1.1",
    "puppeteer": "^23.11.1",
    "react": "^19.2.0",
    "sharp": "^0.34.3",
    "uuid": "^13.0.0",
    "wav": "^1.0.2",
    "xlsx": "^0.18.5",
    "yauzl": "^3.2.0",
    "zod": "^4.1.12"
