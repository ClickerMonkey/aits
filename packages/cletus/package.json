  "devDependencies": {
    "@types/diff": "^7.0.2",
    "@types/file-type": "^10.6.0",
    "@types/node": "^24.9.1",
    "@types/pdf-parse": "^1.1.5",
    "@types/react": "^19.2.2",
    "@types/uuid": "^10.0.0",
    "@types/wav": "^1.0.4",
    "esbuild": "^0.26.0",
    "tsx": "^4.20.6",
    "typescript": "^5.9.3"
  },
  "publishConfig": {
    "access": "public"
  },
  "engines": {
    "node": ">=16.0.0"
  },
  "homepage": "https://github.com/ClickerMonkey/aeye#readme",
  "bugs": {
    "url": "https://github.com/ClickerMonkey/aeye/issues"
  }
