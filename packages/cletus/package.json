{
  "name": "@aeye/cletus",
  "version": "0.3.0",
  "description": "Interactive CLI demo showcasing @aeye capabilities",
  "type": "module",
  "main": "dist/index.js",
  "types": "dist/index.d.ts",
  "bin": {
    "cletus": "dist/index.js"
  },
  "scripts": {
    "build": "npm run clean && node esbuild.config.cjs",
    "dev": "tsx watch src/index.tsx",
    "start": "tsx --conditions=source src/index.tsx",
    "clean": "rimraf dist tsconfig.tsbuildinfo",
    "typecheck": "tsc --noEmit",
    "test": "jest",
    "prepublishOnly": "npm run build",
    "postinstall": "echo '\nCletus installed! Run \"cletus\" to start.\nFor first-time setup, you will need API keys for AI providers.\nSee: https://github.com/ClickerMonkey/aeye#readme\n'"
  },
  "keywords": [
    "cli",
    "ai",
    "demo",
    "interactive"
  ],
  "author": "ClickerMonkey",
  "license": "GPL-3.0",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/ClickerMonkey/aeye.git",
    "directory": "packages/cletus"
  },
  "files": [
    "dist",
    "README.md",
    "LICENSE"
  ],
  "dependencies": {
    "cli-highlight": "^2.1.11",
    "fastembed": "^2.0.0",
    "ink": "^6.4.0",
    "ink-big-text": "^2.0.0",
    "ink-gradient": "^3.0.0",
    "ink-select-input": "^6.2.0",
    "ink-text-input": "^6.0.0",
    "mic": "^2.1.2",
    "react": "^19.2.0",
    "sharp": "^0.34.3"
  },
  "optionalDependencies": {
    "puppeteer": "^24.31.0"
  },
