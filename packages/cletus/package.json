    "@aeye/models": "0.3.0",
    "@aeye/openai": "0.3.0",
    "@aeye/openrouter": "0.3.0",
    "@aeye/replicate": "0.3.0",
    "@babel/preset-env": "^7.26.0",
    "@jest/globals": "^29.7.0",
    "@langchain/core": "^0.3.79",
