{
  "name": "@aeye/models",
  "version": "0.3.2",
  "description": "Static model information scraped from OpenAI, OpenRouter, Replicate, and AWS Bedrock",
  "type": "module",
  "main": "dist/src/index.js",
  "types": "dist/src/index.d.ts",
  "scripts": {
    "build": "tsc",
    "typecheck": "tsc --noEmit",
    "clean": "rimraf dist tsconfig.tsbuildinfo",
    "scrape": "tsx scripts/scrape.ts",
    "scrape:openrouter": "tsx scripts/scrapers/openrouter.ts",
    "scrape:openrouter:full": "tsx scripts/scrapers/openrouter.ts --metrics",
    "scrape:openai": "tsx scripts/scrapers/openai.ts",
    "scrape:replicate": "tsx scripts/scrapers/replicate.ts",
    "scrape:replicate:test": "tsx scripts/scrapers/replicate.ts --transformers --transformers-only --models=\"adirik/kosmos-g\"",
    "scrape:replicate:chunk": "tsx scripts/scrapers/replicate.ts --transformers --transformers-only --transformer-model=\"google/gemini-3-pro-preview\" --n=50 --transform-concurrency=0",
    "scrape:replicate:models": "tsx scripts/scrapers/replicate.ts --transformers-only",
    "scrape:replicate:patch": "tsx scripts/patch-modelinfo.ts",
    "scrape:aws": "tsx scripts/scrapers/aws.ts"
  },
  "keywords": [
    "ai",
    "models",
    "openai",
    "openrouter",
    "replicate",
    "aws",
    "bedrock"
  ],
  "author": "ClickerMonkey",
  "license": "GPL-3.0",
  "repository": {
    "type": "git",
    "url": "https://github.com/ClickerMonkey/aeye.git",
    "directory": "packages/models"
  },
  "dependencies": {
    "@aeye/ai": "0.3.2",
    "@aeye/aws": "0.3.2",
    "@aeye/openrouter": "0.3.2",
    "@aeye/replicate": "0.3.2"
  },
  "devDependencies": {
    "@aws-sdk/client-bedrock": "^3.709.0",
    "@types/node": "^20.12.7",
    "axios": "^1.7.2",
    "openapi-typescript": "^7.10.1",
    "puppeteer": "^24.31.0",
    "replicate": "^0.34.1",
    "tsconfig-paths": "^4.2.0",
    "tsx": "^4.7.0",
    "typescript": "^5.9.3"
  },
  "publishConfig": {
    "access": "public"
  },
  "engines": {
    "node": ">=16.0.0"
  },
  "homepage": "https://github.com/ClickerMonkey/aeye#readme",
  "bugs": {
    "url": "https://github.com/ClickerMonkey/aeye/issues"
  }
}
